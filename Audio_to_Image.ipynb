        "def plot_teste_rgb(width=0, height=0, signal=None):\n",
        "    image = Image.new(\"RGB\", (width, height), color=0)\n",
        "    for x in range(width):\n",
        "        for y in range(height):\n",
        "            c = int(signal[(x + y*width)%256])\n",
        "            image.putpixel((x, y), (c, signal[x], signal[x]))\n",
        "    return image"
